{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2023"],
    "module": "Node16",

    "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "moduleResolution": "Node16"
  },
  "include": ["."],
  "exclude": ["node_modules", "coverage"]
}
